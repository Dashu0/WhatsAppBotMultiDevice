{
  "name": "whatsapp-bot-md",
  "description": "nodejs whatsapp bot",
  "logo": "https://i.ibb.co/sWmwD0k/e0bslrup.jpg",
  "repository": "https://github.com/jacktheboss220/WhatsAppBotMultiDevice",
  "keywords": [
    "whatsapp-bot",
    "insta",
    "multi-device"
  ],
  "env": {
    "PREFIX": {
      "description": "Prefix for the bot"
    },
    "REMOVE_BG_KEY": {
      "description": "Get from remove.bg"
    },
    "DEEPAI_KEY": {
      "description": "Get your api key from deepai.org"
    },
    "myNumber": {
      "description": "Enter your number to get all update (include 91 or country code)"
    },
    "botNumber": {
      "description": "Enter bot number to get access to all cmd (include 91 or country code)"
    },
    "INSTA_API_KEY": {
      "description": "Enter insta session key. Get from inspecting your insta session cookie"
    },
    "TRUECALLER_ID": {
      "description": "Enter true caller id. Get form https://www.npmjs.com/package/truecallerjs"
    },
    "MONGODB_KEY": {
      "description": "Enter the mongodb uri"
    },
    "GOOGLE_API_KEY": {
      "description": "Enter your Google API key"
    },
    "SEARCH_ENGINE_KEY": {
      "description": "Enter your search engine key"
    },
    "OPENAI_API_KEY": {
      "description": "Enter your OpenAI API key"
    },
    "GENIUS_ACCESS_SECRET": {
      "description": "Enter your Genius API key"
    },
    "HOST_URL": {
      "description": "Enter your host URL after hosting the bot"
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs#latest"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
    }
  ],
  "addons": []
}
